#############################
# Basic CACHE parameters(max levels = 3)
#############################

# System bus width in Bytes
CACHE.bus=4

# Page size in KBytes
CACHE.page_size=256

# Cache levels
CACHE.levels=3

# Write policy for all levels
CACHE.write_policy=0

# Replaycement policy
CACHE.replaycement=1

# Associativity
CACHE.associativity=1

#############################
# Cache L1 parameters
#############################

# Cache size in KBytes
L1.size=256

# Associativity sets
L1.sets=4

#############################
# Cache L2 parameters
#############################

# Cache size in KBytes
L2.size=256

# Associativity sets
L2.sets=4

#############################
# Cache L3 parameters
#############################

# Cache size in MBytes
L3.size=256

# Associativity sets
L3.sets=4

#############################
# DDR top parameters(max channels = 4)
#############################

# DDR channels
DDR.channels=2

# Interleaving policy
DDR.interleaving=0

#############################
# DDR channel 0 parameters
#############################

# Number of DIMMs in channel
DDR.0.dimms=4

# Namber BANKs in dimms
DDR.0.banks=16

#############################
# DDR channel 1 parameters
#############################

# Number of DIMMs in channel
DDR.1.dimms=4

# Namber BANKs in dimms
DDR.1.banks=16

#############################
# DDR channel 2 parameters
#############################

# Number of DIMMs in channel
DDR.2.dimms=4

# Namber BANKs in dimms
DDR.2.banks=16

#############################
# DDR channel 3 parameters
#############################

# Number of DIMMs in channel
DDR.3.dimms=4

# Namber BANKs in dimms
DDR.3.banks=16